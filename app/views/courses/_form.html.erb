<%= simple_form_for(@course) do |f| %>
  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :professor %>
    <%= f.input :code %>
    <%= f.input :description %>
  </div>

  <div class="form-actions mt-3">
    <%= f.submit "Create Course", type: "button", class: "btn btn-outline-primary" %>
  </div>
<% end %>

